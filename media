#include<iostream>
media(int a, int b){
            float resultado;
            resultado=(a+b)/2.0;
            return resultado;
}
int main(){
    float a,b,y;
    std::cout<< "Dime el valor de a";
    std::cin>> a;
    std::cout<< "Dime el valor de b";
    std::cin>> b;
    media (a,b);
    std::cout<< "La media vale";
    std::cout<< media;
    std::cin>>y;
    return 0;
    
    
}
    
