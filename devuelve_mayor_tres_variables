#include <iostream>
int devuelvemayor (int a, int b, int c){
    int resp;
    if (a>b){
              if (a>c){
                  resp=a;
                         }
             }
    if (b>a){
             if (b>c){
                  resp=b;
                         }
             }
    if (c>a){
             if (c>b){
                 resp=c;
                        }
             }          
    return resp;    
}
int main(){
    int a,b,c;
    int x;
    std::cout<<"Dime un numero";
    std::cin>>a;
    std::cout<<"Dime otro numero";
    std::cin>>b;
    std::cout<<"Dime un numerito mass";
    std::cin>>c;
    std::cout<< devuelvemayor(a,b,c);
    std::cin>>x;
    return 0;
}
